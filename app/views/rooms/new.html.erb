<%= form_with(model: @room, local: true) do |f| %>
<%# 入力に不備があった場合、エラーメッセージを表示する %>
<%= render 'shared/error_messages', model: f.object %>  
  <form>
    <div class="form-group">
      <label>ルーム名</label>
      <%= f.text_field :name, class: 'form-control'%>
    </div>
    <div class="form-group text-center">
      <%= f.submit "作成する" %>
    </div>
  </form>
<%end%>