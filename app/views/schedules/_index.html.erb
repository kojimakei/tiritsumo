<h1>今週の予定</h1>

  <div id='calendar'>
  </div>
  

<table>
  <tbody>
    <% @schedules.each do |schedule| %>
    <% end %>
  </tbody>
</table>

<div>
  <%= week_calendar events: @schedules do |date, schedules| %>
    <%= date.day %>
      <% schedules.each do |schedule| %>
      <li><%= schedule.plan %></li>
  <% end %>
<% end %>

</div>
